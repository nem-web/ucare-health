# App Configuration
VITE_APP_NAME=Ucare Health Tracker
VITE_APP_VERSION=1.0.0
VITE_APP_URL=https://your-app.vercel.app

# MongoDB Configuration (Required for cloud sync)
VITE_MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=YourApp
VITE_MONGODB_DB_NAME=ucare_health
VITE_MONGODB_API_KEY=your_mongodb_data_api_key

# Firebase Configuration (Optional - for authentication)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
